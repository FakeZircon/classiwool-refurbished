{
	"schemaVersion": 1,
	"id": "classiwool-refurbished",
	"version": "${version}",
	"name": "Classiwool Refurbished",
	"description": "A mod that re-adds the classic wool colours alongside their modern dyeable counterparts!",
	"authors": [
		"FakeZircon",
		"Fitzs_Space"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/classiwool-refurbished/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"classiwool_refurbished.ClassiwoolRefurbished"
		],
		"client": [
			"classiwool_refurbished.ClassiwoolRefurbishedClient"
		],
		"fabric-datagen": [
			"classiwool_refurbished.ClassiwoolRefurbishedDataGenerator"
		]
	},
	"mixins": [
		"classiwool-refurbished.mixins.json",
		{
			"config": "classiwool-refurbished.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.13",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}